{
  "manifest_version": 3,
  "name": "Leads generation",
  "version": "1.0",
  "description": "Generate leads",
  "permissions": ["scripting"],
  "host_permissions": [
    "https://www.youtube.com/*",
    "http://localhost/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["youtube_helper.js", "apis.js", "content.js", "helper.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ]
}
